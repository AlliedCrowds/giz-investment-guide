{
  "name": "make-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:dev": "yarn run clean:dist:dev && cross-env NODE_ENV=development webpack --config webpack.prod.ts",
    "build:prod": "yarn run clean:dist:prod && cross-env NODE_ENV=production webpack --config webpack.prod.ts",
    "watch": "webpack-dev-server --config webpack.dev.ts --open",
    "clean:dist:dev": "rimraf ./dist/develpoment",
    "clean:dist:prod": "rimraf ./dist/production",
    "clean:dist": "rimraf ./dist",
    "clean:node_modules": "rimraf ./node_modules",
    "clean:all": "npm-run-all -p clean:dist clean:node_modules",
    "lint": "eslint ./src",
    "prettier": "pretty-quick --staged",
    "postinstall": "yarn run build:prod",
    "deploy:dev": "yarn run build:dev && yarn run publish:dev",
    "deploy:prod": "yarn run build:prod && yarn run publish:prod",
    "publish:dev": "gh-pages -t -d ./dist/development -r git@github.com:AlliedCrowds/dev-investment-guide.git -b master",
    "publish:prod": "gh-pages -t -d ./dist/production -r git@github.com:AlliedCrowds/investment-guide.git -b master"
  },
  "dependencies": {
    "@alliedcrowds/capital-finder-vuex": "^0.2.2",
    "copy-webpack-plugin": "^5.0.3",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.16.4",
    "gh-pages": "^2.0.1",
    "i": "^0.3.6",
    "marked": "^0.5.2",
    "npm": "^6.7.0",
    "serve-static": "^1.13.2",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/marked": "^0.5.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.12.12",
    "@types/webpack": "^4.4.21",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-merge": "^4.1.3",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.10.0",
    "eslint-plugin-vue": "^5.0.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-url-loader": "^2.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all prettier lint"
    }
  }
}
